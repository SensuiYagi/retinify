Checks: >
  -*,
  bugprone-*,
  cert-*,
  cppcoreguidelines-*,
  hicpp-*,
  performance-*,
  readability-*,
  misra-*,
  clang-analyzer-*,
  modernize-*,
  portability-*,
  llvm-include-order
WarningsAsErrors: ''
HeaderFilterRegex: '.*retinify/.*'
CheckOptions:
  - { key: readability-identifier-naming.ClassCase, value: CamelCase }
  - { key: cppcoreguidelines-narrowing-conversions.WarnOnIntegerNarrowingConversion, value: true }
